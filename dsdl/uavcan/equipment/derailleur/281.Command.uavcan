#
# Nested type.
# Single derailleur command.
#

uint4 derailleur_id

#
# Whether the units are linear or angular depends on the actuator type.
#
uint4 COMMAND_TYPE_RELATIVE     = 0     # [-1, +1]
uint4 COMMAND_TYPE_ABSOLUTE     = 1     # index position
uint4 command_type

#
# Value of the above type
#
int8 command_value
